<xs:schema xmlns:eu="http://factura.envasesuniversales.com/addenda/eu" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://factura.envasesuniversales.com/addenda/eu" version="1.5" xmlns:xs="http://www.w3.org/2001/XMLSchema">
